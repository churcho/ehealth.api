{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "type": {
        "type": "string"
      },
      "edrpou": {
        "type": "string"
      },
      "status": {
        "type": "string",
        "enum": [
          "ACTIVE",
          "CLOSED"
        ]
      },
      "nhs_verified": {
        "type": "boolean"
      }
    },
    "required": [
      "id",
      "type",
      "edrpou",
      "status"
    ]
  }
}
